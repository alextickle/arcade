<html>
<head>
  <title>Black Jack</title>
  <link rel="stylesheet" href="/css/bootstrap/bootstrap.css">
  <link rel="stylesheet" href="/css/bootstrap/bootstrap-theme.css">
  <link href="/css/blackJack.css" type="text/css" rel="stylesheet"/>
  <link href="https://fonts.googleapis.com/css?family=Diplomata+SC" rel="stylesheet">
  <script src="/js/jquery-3.1.1.js"></script>
  <script src="/js/blackJack.js"></script>
</head>
<body>
  <div class="container">
    <div class="row">
      <div id="title" class="col-sm-12">
        <h3>Black Jack</h3>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <h5>DEALER</h5>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-2">
        <img id="d0" src="">
      </div>
      <div class="col-sm-2">
        <img id="d1" src="">
      </div>
      <div class="col-sm-2">
        <img id="d2" src="">
      </div>
      <div class="col-sm-2">
        <img id="d3" src="">
      </div>
      <div class="col-sm-2">
        <img id="d4" src="">
      </div>
      <div class="col-sm-2">
        <img id="d5" src="">
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <h5>PLAYER</h5>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-2">
        <img id="p0" src="">
      </div>
      <div class="col-sm-2">
        <img id="p1" src="">
      </div>
      <div class="col-sm-2">
        <img id="p2" src="">
      </div>
      <div class="col-sm-2">
        <img id="p3" src="">
      </div>
      <div class="col-sm-2">
        <img id="p4" src="">
      </div>
      <div class="col-sm-2">
        <img id="p5" src="">
      </div>
    </div><br>
    <div id="header" class="row">
      <div class="col-sm-5">
        <p id="dealerComment"></p>
      </div>
      <div class="col-sm-5">
        <button class="active" id="start" type="button">Deal</button>
        <button class="inactive" id="hit" type="button">Hit</button>
        <button class="inactive" id="stay" type="button">Stay</button>
      </div>
      <div class="col-sm-2">
        <button class="inactive" id="replay" type="button"><a href="blackJack.html">Play Again</a></button>
      </div>
    </div><br>
    <div id="footer" class="row">
      <p><a href="/">Home</a></p>
    </div>
  </div>
</body>
</html>
